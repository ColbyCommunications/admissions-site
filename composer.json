        "colbycommunications/wp-plugin-acf-pro": "^5.11.4",
        "colbycommunications/wp-plugin-elementor-pro": "^3.5.2",
        "colbycommunications/wp-plugin-jet-blocks-pro": "^1.3.0",
        "colbycommunications/wp-plugin-jet-blog-pro": "^2.2.15",
        "colbycommunications/wp-plugin-jet-elements-pro": "^2.6.2",
        "colbycommunications/wp-plugin-jet-menu-pro": "^2.1.6",
        "colbycommunications/wp-plugin-jet-popup-pro": "^1.5.6",
        "colbycommunications/wp-plugin-jet-search-pro": "^2.1.17",
        "colbycommunications/wp-plugin-jet-tabs-pro": "^2.1.15",
