{
    "name": "colby-communications/admissions.colby.edu",
    "description": "Configuration project for the admissions.colby.edu site",
    "type": "project",
    "require": {
        "johnpbloch/wordpress-core-installer": "^1.0",
        "johnpbloch/wordpress-core": "^5.1.1",
        "wpackagist-theme/twentyseventeen": "^2.1",
        "wpackagist-plugin/classic-editor": "^1.4",
        "wpackagist-plugin/super-simple-account-enumeration-blocker": "^1.0.0",
        "colbycommunications/colby-afa-theme": "dev-dev",
        "colbycommunications/colby-groups": "dev-feature/platform",
        "colbycollege/colby-lando": "dev-dev"
    },
    "authors": [
        {
            "name": "Morty Mule",
            "email": "bkwaltz@colby.edu"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org/"
        },
        {
            "type": "git",
            "url": "https://github.com/ColbyCommunications/colby-groups"
        },
        {
            "type": "git",
            "url": "https://github.com/ColbyCommunications/colby-afa-theme"
        },
        {
            "type": "git",
            "url": "https://github.com/ColbyCommunications/colby-lando"
        }
    ],
    "extra": {
        "wordpress-install-dir": "web/wp",
        "installer-paths": {
            "web/wp-content/mu-plugins/{$name}": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/wp-saml-auth",
                "wpackagist-plugin/super-simple-account-enumeration-blocker"
            ],
            "web/wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ],
            "web/wp-content/themes/{$name}": [
                "type:wordpress-theme"
            ]
        }
    }
}
