{
    "name": "colby-college/site",
    "description": "Configuration project for the site-somesite.colby.edu site",
    "type": "project",
    "require": {
        "simplesamlphp/simplesamlphp": "^1.17",
        "johnpbloch/wordpress-core-installer": "^1.0",
        "johnpbloch/wordpress-core": "^5.1.1",
        "wpackagist-plugin/wp-saml-auth": ">=0.5.1 <1.0.0",
        "wpackagist-theme/twentyseventeen": "^2.1",
        "wpackagist-plugin/classic-editor": "^1.4",
        "wpackagist-plugin/super-simple-account-enumeration-blocker": "^1.0.0",
        "wpackagist-plugin/cache-control": "^2.2.3",
        "colbycommunications/colby-groups": "dev-wp4-test",
        "wpackagist-plugin/capability-manager-enhanced": "1.9",
        "wpackagist-plugin/tablepress": "1.10"
    },
    "authors": [
        {
            "name": "Morty Mule",
            "email": "webmaster@colby.edu"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/ColbyCommunications/colby-groups"
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org/"
        }
    ],
    "extra": {
        "wordpress-install-dir": "web/wp",
        "installer-paths": {
            "web/wp-content/mu-plugins/{$name}": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/wp-saml-auth",
                "wpackagist-plugin/super-simple-account-enumeration-blocker",
                "wpackagist-plugin/cache-control",
                "forumone/wp-cfm",
                "mizzou/wp-cfm-config"
            ],
            "web/wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ],
            "web/wp-content/themes/{$name}": [
                "type:wordpress-theme"
            ]
        }
    }
}
