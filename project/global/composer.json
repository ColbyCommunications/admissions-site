{
    "name": "colby-college/platform-wp-starter",
    "description": "Starter template for the colby theme",
    "type": "project",
    "require": {
        "johnpbloch/wordpress-core-installer": "^2.0",
        "johnpbloch/wordpress-core": "6.8.1",
        "wpackagist-plugin/header-footer": "3.3.2",
        "wpackagist-plugin/duplicate-post": "4.5",
        "wpackagist-plugin/wordpress-seo": "25.3.1",
        "wpackagist-plugin/cloudflare": "4.12.8",
        "wpackagist-plugin/public-post-preview": "3.0.1",
        "wpackagist-plugin/wp-saml-auth": "2.2.0",
        "wp-cli/wp-cli-bundle": "^2.4",
        "psy/psysh": "^0.10.4",
        "colbycommunications/advanced-custom-fields-pro": "6.4.2",
        "colbycommunications/gravityforms": "2.9.11",
        "colbycommunications/colby-ticket": "^1.0.0",
        "wpackagist-plugin/wp-search-with-algolia": "2.10.1",
        "vlucas/phpdotenv": "^5.4",
        "colbycommunications/colby-college-theme": "3.5.3",
        "timber/timber": "2.3.1",
        "wpackagist-plugin/regenerate-thumbnails": "3.1.6",
        "wpackagist-plugin/disable-comments": "2.5.2",
        "colbycommunications/capabilities-pro": "2.19.2",
        "wpackagist-plugin/wp-nested-pages": "3.2.13",
        "wpackagist-plugin/user-switching": "1.9.1",
        "colbycommunications/presspermit-pro": "4.3.2",
        "wpackagist-plugin/advanced-gutenberg": "3.2.4"
    },
    "authors": [
        {
            "name": "Morty Mule",
            "email": "webmaster@colby.edu"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org/"
        },
        {
            "type": "composer",
            "url": "https://satis.colby.edu"
        },
        {
            "type": "git",
            "url": "https://github.com/ColbyCommunications/colby-college-theme"
        }
    ],
    "extra": {
        "wordpress-install-dir": "web/wp",
        "installer-paths": {
            "web/wp-content/mu-plugins/{$name}": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/wordpress-seo"
            ],
            "web/wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ],
            "web/wp-content/themes/{$name}": [
                "type:wordpress-theme"
            ]
        }
    },
    "config": {
        "allow-plugins": true
    }
}
